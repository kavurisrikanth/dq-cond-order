(DataQuery SortedCustomersUsingInput2 {
    inputs [
        (sortBy {
            type ({
                optionSet #CustomerSortOptions
            })
        })
    ]
    query `
        Customer.all.orderBy((c) => inputs.sortBy == AGE ? (c.isUnderAge ? c.guardian.ageInYears.toString() : c.ageInYears.toString()) : (inputs.sortBy == NAME ? c.name : c.ageInYears.toString()))
    `
})