(Model Customer {
    creatable true
    properties [
        (name {
            type ({
                primitive String
            })
            required true
        })
        (dob {
            type ({
                primitive DateTime
            })
            required true
        })
        (yob {
            type ({
                primitive Integer
            })
            readType None
        })
        (isUnderAge {
            type ({
                primitive Boolean
            })
            computed true
            computation `CustomerUtil.isUnderAge(this)`
        })
        (guardian {
            type ({
                model #Customer
            })
            existsIf `this.isUnderAge`
            required true
        })
    ]
})